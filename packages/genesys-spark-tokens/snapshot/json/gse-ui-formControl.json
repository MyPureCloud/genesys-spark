{
  "form_control": {
    "input": {
      "content_text": {
        "fontFamily": "Noto Sans",
        "lineHeight": "18px",
        "fontSize": "12px",
        "fontWeight": 400
      },
      "default": {
        "border": {
          "color": "#a3a7b0",
          "width": "1px",
          "style": "solid"
        }
      },
      "hover": {
        "border": {
          "color": "#3742A2",
          "width": "1px",
          "style": "solid"
        }
      },
      "active": {
        "border": {
          "color": "#252E78",
          "width": "1px",
          "style": "solid"
        }
      },
      "error": {
        "border": {
          "color": "#e22245",
          "width": "1px",
          "style": "solid"
        }
      },
      "disabled": {
        "border": {
          "color": "#a3a7b0",
          "width": "1px",
          "style": "solid"
        },
        "opacity": 0.5
      },
      "border_radius": "4px",
      "background_color": "#ffffff",
      "placeholder_color": "#626e84",
      "populated_color": "#1b2c48",
      "input_clearable": {
        "input_clearable_color": "#626e84",
        "size": "16px"
      },
      "gap": "12px",
      "padding": "0 12px",
      "prefix_sufix": {
        "text": {
          "fontFamily": "Noto Sans",
          "lineHeight": "20px",
          "fontSize": "14px",
          "fontWeight": 700
        },
        "height": "32px",
        "default_color": "#626e84"
      },
      "textfield": {
        "height": "32px"
      },
      "input_icon": {
        "size": "16px",
        "default_color": "#3e5374",
        "icon_end_color": "#626e84"
      },
      "focus_ring": {
        "default_color": "#7B88F7"
      },
      "textarea": {
        "height": "96px"
      },
      "focus": {
        "border": {
          "color": "#7B88F7",
          "width": "3px",
          "style": "solid"
        }
      },
      "spinbutton_icon": {
        "size": "16px"
      },
      "focus_selection": {
        "background_color": "#7B88F7"
      }
    },
    "label": {
      "padding": "0 0 8px 0",
      "gap": "4px",
      "text": {
        "fontFamily": "Noto Sans",
        "lineHeight": "18px",
        "fontSize": "12px",
        "fontWeight": 400
      },
      "label_color": "#1b2c48",
      "indicator": {
        "text": {
          "fontFamily": "Noto Sans",
          "lineHeight": "18px",
          "fontSize": "12px"
        },
        "required_color": "#e22245",
        "optional_color": "#626e84"
      }
    },
    "helper": {
      "gap": "8px",
      "helper_text": {
        "fontFamily": "Noto Sans",
        "lineHeight": "18px",
        "fontSize": "12px",
        "fontWeight": 400
      },
      "padding": "8px 0 0 0",
      "error_padding": "4px 0 0 0",
      "default_color": "#626e84",
      "error_color": "#e22245",
      "icon_size": "16px"
    },
    "spin_button": {
      "gap": "8px"
    },
    "spinner": {
      "track": "#4856CB",
      "error_color": "#CED2FC"
    },
    "textarea": {
      "padding": "8px 12px"
    },
    "focus_ring": {
      "border_radius": "4px"
    }
  }
}
