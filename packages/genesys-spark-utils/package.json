{
  "name": "genesys-spark-utils",
  "version": "4.154.7",
  "description": "Utility functions for Genesys Spark components",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "npm run prebuild.clean",
    "build": "npm run build.rollup",
    "build.rollup": "rollup -c",
    "dev": "rollup -c --watch",
    "eslint": "eslint . --fix",
    "lint-all": "npm-run-all \"eslint\" \"prettier\" \"prettier-package-json\"",
    "lint-staged": "lint-staged --concurrent false",
    "prebuild.clean": "./scripts/clean.sh",
    "prettier": "prettier --log-level silent --ignore-path .prettierignore --ignore-unknown --write .",
    "prettier-package-json": "prettier-package-json --write ./package.json",
    "test": "jest",
    "test.ci": "npm run test",
    "test.unit": "jest",
    "test.unit.watch": "jest --watch",
    "version-sync": "npm version --no-git-tag-version --allow-same-version"
  },
  "types": "dist/index.d.ts",
  "devDependencies": {
    "@rollup/plugin-replace": "^6.0.2",
    "@tsconfig/strictest": "^2.0.5",
    "eslint": "^9.29.0",
    "eslint-config-genesys-spark-components": "file:../../shared-configs/eslint-config-genesys-spark-components",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "prettier-config-genesys-spark-components": "file:../../shared-configs/prettier-config-genesys-spark-components",
    "prettier-package-json": "^2.8.0",
    "rollup": "^4.44.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-esbuild": "^6.2.1",
    "ts-jest": "^29.4.0",
    "typescript": "5.5.4"
  },
  "prettier": "prettier-config-genesys-spark-components"
}
